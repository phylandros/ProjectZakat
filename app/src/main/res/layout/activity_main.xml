<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".MainActivity"
    android:background="#FFFFFF"
    android:orientation="vertical">

        <LinearLayout
            android:id="@+id/rectangle_7"
            android:layout_width="match_parent"
            android:layout_height="295dp"
            android:layout_alignParentLeft="true"
            android:layout_alignParentTop="true"
            android:layout_marginTop="-45dp"
            android:orientation="vertical"
            android:background="@drawable/rectangle_7"
            >

            <TextView
                android:id="@+id/hi_"
                android:layout_width="wrap_content"
                android:layout_height="24dp"
                android:layout_alignParentLeft="true"
                android:layout_marginLeft="47dp"
                android:layout_alignParentTop="true"
                android:layout_marginTop="125dp"
                android:text="@string/hi_"
                android:textAppearance="@style/hi_"
                android:gravity="top"
                />

            <TextView
                android:id="@+id/nama"
                android:layout_width="68dp"
                android:layout_height="29dp"
                android:layout_alignParentLeft="true"
                android:layout_marginLeft="47dp"
                android:layout_alignParentTop="true"
                android:text="@string/nama"
                android:textAppearance="@style/nama"
                android:gravity="top"
                />
        </LinearLayout>

    <Button
        android:id="@+id/logoutmain"
        android:layout_width="119dp"
        android:layout_height="26dp"
        android:layout_alignParentLeft="true"
        android:layout_marginLeft="288dp"
        android:layout_alignParentTop="true"
        android:layout_marginTop="11dp"
        android:text="Logout"
        android:textColor="#000000"
        android:textStyle="bold"
        android:background="@drawable/rectangle_2"
        />

    <View
        android:id="@+id/banner"
        android:layout_width="334dp"
        android:layout_height="143dp"
        android:layout_alignParentLeft="true"
        android:layout_marginLeft="40dp"
        android:layout_alignParentTop="true"
        android:layout_marginTop="179dp"
        android:background="@drawable/rectangle_8"
        />

    <View
        android:id="@+id/jmluangbox"
        android:layout_width="160dp"
        android:layout_height="210dp"
        android:layout_alignParentLeft="true"
        android:layout_marginLeft="40dp"
        android:layout_alignParentTop="true"
        android:layout_marginTop="352dp"
        android:background="@drawable/rectangle_1"
        />

    <View
        android:id="@+id/jmlberasbox"
        android:layout_width="160dp"
        android:layout_height="210dp"
        android:layout_alignParentLeft="true"
        android:layout_marginLeft="214dp"
        android:layout_alignParentTop="true"
        android:layout_marginTop="352dp"
        android:background="@drawable/rectangle_1"
        />

    <Button
        android:id="@+id/uangbox"
        android:layout_width="102dp"
        android:layout_height="92dp"
        android:layout_alignParentLeft="true"
        android:layout_marginLeft="40dp"
        android:layout_alignParentTop="true"
        android:layout_marginTop="584dp"
        android:text="Uang"
        android:textStyle="bold"
        android:textColor="#000000"
        android:background="@drawable/rectangle_1"
        />

    <Button
        android:id="@+id/berasbox"
        android:layout_width="102dp"
        android:layout_height="92dp"
        android:layout_alignParentLeft="true"
        android:layout_marginLeft="156dp"
        android:layout_alignParentTop="true"
        android:layout_marginTop="584dp"
        android:text="Beras"
        android:textStyle="bold"
        android:textColor="#000000"
        android:background="@drawable/rectangle_1"
        />

    <Button
        android:id="@+id/aboutbox"
        android:layout_width="102dp"
        android:layout_height="92dp"
        android:layout_alignParentLeft="true"
        android:layout_marginLeft="272dp"
        android:layout_alignParentTop="true"
        android:layout_marginTop="584dp"
        android:text="About"
        android:textStyle="bold"
        android:textColor="#000000"
        android:background="@drawable/rectangle_1"
        />


</RelativeLayout>